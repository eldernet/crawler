{
  "name": "eldernet/crawler",
  "description": "Crawl all internal links found on a website",
  "keywords": [
    "spatie",
    "crawler",
    "link",
    "website"
  ],
  "homepage": "https://github.com/eldernet/crawler",
  "license": "MIT",
  "authors": [
    {
      "name": "G Brabyn"
    }
  ],
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^2.0",
    "illuminate/collections": "^10.0",
    "nicmart/tree": "^0.3.0",
    "spatie/browsershot": "^3.45",
    "spatie/robots-txt": "^2.0",
    "symfony/dom-crawler": "^6.0"
  },
  "require-dev": {
    "pestphp/pest": "^2.0",
    "spatie/ray": "^1.37"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "Eldernet\\Crawler\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eldernet\\Crawler\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
